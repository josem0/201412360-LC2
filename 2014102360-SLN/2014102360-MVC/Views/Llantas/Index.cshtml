@model IEnumerable<_2014102360_ENT.Llanta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Llantas</h2>

<p>
    @Html.ActionLink("Nueva Llanta", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumSerie)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerie)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LlantaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.LlantaId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.LlantaId })
        </td>
    </tr>
}

</table>
