@model IEnumerable<_2014102360_ENT.Cinturon>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cinturones</h2>

<p>
    @Html.ActionLink("Nuevo Cinturon", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumSerieCinturon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Metraje)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerieCinturon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Metraje)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CinturonId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CinturonId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CinturonId })
        </td>
    </tr>
}

</table>
