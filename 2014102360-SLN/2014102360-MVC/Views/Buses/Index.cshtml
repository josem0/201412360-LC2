@model IEnumerable<_2014102360_ENT.Bus>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Buses</h2>

<p>
    @Html.ActionLink("Nuevo Bus", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Asiento.NumSerieAsiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ensambladora._Ensambladora)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Llanta.NumSerie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Parabrisas.NumSerie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Propietario.DNI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Volante.NumSerie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumSerieMotor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumSerieChasis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoCarro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoBus)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Asiento.NumSerieAsiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ensambladora._Ensambladora)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Llanta.NumSerie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parabrisas.NumSerie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Propietario.DNI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volante.NumSerie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerieMotor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumSerieChasis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoCarro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoBus)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CarroId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CarroId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CarroId })
        </td>
    </tr>
}

</table>
